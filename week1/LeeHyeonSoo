#1

num1, num2 = input().split()

num1 = int(num1)
num2 = int(num2)

if(num1 > num2):
    print(">")

elif(num1 < num2):
    print("<")

else:
    print("==")



#2
    num = int(input())

for i in range(1, num+1):
    print("*"*i)







#3
num = int(input())
num_list = list(map(int, input().split()))

print(str(min(num_list)), str(max(num_list)))





#4
num1, num2 = input().split()

num1 = int(num1[::-1])
num2 = int(num2[::-1])

if(num1 > num2):
    print(num1)
else:
    print(num2)
